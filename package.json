    "@chakra-ui/react": "^2.4.6",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@reduxjs/toolkit": "^1.9.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "chakra-react-select": "^4.6.0",
    "date-fns": "^2.30.0",
    "dynamoose": "^3.2.0",
    "eslint": "^8.31.0",
    "eslint-config-next": "^13.2.4",
    "final-form": "^4.20.9",
    "final-form-calculate": "^1.3.2",
    "framer-motion": "^6",
    "google-auth-library": "^8.7.0",
    "googleapis": "^110.0.0",
    "jwt-decode": "^3.1.2",
    "loadash": "^1.0.0",
    "next": "^13.5.6",
    "next-auth": "^4.23.2",
    "next-international": "^1.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-final-form": "^6.5.9",
    "react-icons": "^4.7.1",
    "react-imask": "^6.6.0",
    "react-redux": "^8.0.5",
    "react-table": "^7.8.0",
    "redux-persist": "^6.0.0",
    "redux-state-sync": "^3.1.4",
    "slugify": "^1.6.6",
    "typescript": "^5.2.2",
    "ulid": "^2.3.0",
    "uuid": "^9.0.1",
    "yup": "^1.2.0"
